{
    "setup": {
        "gpu": {
            "use": true,
            "id": 0
        },
        "model": "GraphSage",
        "train_dataset": "MVC",
        "val_dataset": "MVC",
        "test_dataset": "MVC",
        "out_dir": "out/MVC_node_classification/",
        "features": "basic"
    },

    "tunable_params": {
        "init_lr": 0.01,
        "lr_reduce_factor": 0.5,
        "lr_schedule_patience": 10,
        "min_lr": 0.0005,
        "weight_decay": 0.0005
    },

    "params": {
        "seed": "random",
        "epochs": 1000,
        "print_epoch_interval": 1,
        "max_time": 24
    },

    "tunable_net_params": {
        "batch_size": 64,
        "batch_norm": true,
        "dropout": 0.2,
        "residual": true,
        "loss_weight": [1, 1]
    },

    "net_params": {
        "L": 4,
        "hidden_dim": 45,
        "out_dim": 45,
        "sage_aggregator": "lstm"
    }
}