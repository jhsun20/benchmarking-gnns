{
    "setup": {
        "gpu": {
            "use": true,
            "id": 0
        },
        "model": "GIN",
        "train_dataset": "MIS",
        "val_dataset": "MIS",
        "test_dataset" : "MIS",
        "out_dir": "out/MIS_node_classification/",
        "features": "basic"
    },

    "tunable_params": {
        "init_lr": 0.01,
        "lr_reduce_factor": 0.5,
        "lr_schedule_patience": 25,
        "min_lr": 0.00001,
        "weight_decay": 0.0005
    },

    "params": {
        "seed": "random",
        "epochs": 1000,
        "print_epoch_interval": 1,
        "max_time": 24
    },

    "tunable_net_params": {
        "batch_size": 64,
        "hidden_dim": 120,
        "out_dim": 120,
        "batch_norm": true,
        "dropout": 0.2,
        "residual": true,
        "loss_weight": [1, 1],
        "apply_fn_layers": 2,
        "learn_eps": true,
        "aggr_type": "sum"
    },

    "net_params": {
        "L": 4
    }
}