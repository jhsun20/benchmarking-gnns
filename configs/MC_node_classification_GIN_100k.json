{
    "gpu": {
        "use": true,
        "id": 0
    },

    "model": "GIN",
    "train_dataset": "MC",
    "val_dataset": "MC",

    "out_dir": "out/MC_node_classification/",

    "params": {
        "seed": 41,
        "epochs": 1000,
        "batch_size": 128,
        "init_lr": 1e-2,
        "lr_reduce_factor": 0.5,
        "lr_schedule_patience": 25,
        "min_lr": 1e-5,
        "weight_decay": 5e-4,
        "print_epoch_interval": 5,
        "max_time": 24
    },

    "net_params": {
        "L": 4,
        "hidden_dim": 50,
        "residual": true,
        "readout": "sum",
        "n_mlp_GIN": 2,
        "learn_eps_GIN": true,
        "neighbor_aggr_GIN": "sum",
        "in_feat_dropout": 0.35,
        "dropout": 0.35,
        "batch_norm": true
    }
}